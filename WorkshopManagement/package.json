{
  "name": "supatronix-mvp",
  "private": true,
  "workspaces": [
    "apps/web",
    "apps/api",
    "packages/prisma",
    "packages/shared",
    "scripts"
  ],
  "scripts": {
    "start:web": "cd apps/web && npm run dev",
    "start:api": "cd apps/api && npm run start",
    "seed": "node scripts/seed-data.js",
    "dev": "concurrently \"npm run start:web\" \"npm run start:api\"",
    "seed:extra": "node scripts/seed-extra.js",
    "package-prod": "bash tools/make-prod-zip.sh",
    "build": "echo 'Build as part of pipeline'",
    "lint": "echo 'Lint to be added'"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0"
  }
}
